{
  "name": "@tertium/js",
  "version": "0.16.0",
  "description": "Shared JS configs and types.",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts"
    },
    "./prettierrc": {
      "default": "./.prettierrc"
    },
    "./scripts/release": {
      "default": "./scripts/release.js"
    }
    ,
    "./auth": {
      "types": "./types/auth/index.d.ts",
      "default": "./core/auth/src/index.ts"
    }
    ,
    "./entities": {
      "types": "./types/entities/index.d.ts",
      "default": "./entities/index.ts"
    }
  },
    "files": [
    "types/",
    ".prettierrc",
    "scripts/",
    "core/auth/src/",
    "entities/"
  ],
  "scripts": {
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Vitalii Balabanov",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
